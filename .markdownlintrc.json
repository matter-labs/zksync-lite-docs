{
  "MD013": {
    "indent": 2,
    "tables": false,
    "code_blocks": false,
    "line_length": 300
  },
  "MD033": {
    "no-inline-html": false,
    "allowed_elements": [
      "br",
      "label",
      "sup",
      "p",
      "iframe",
      "img",
      "table",
      "b",
      "span",
      "tr",
      "td",
      "div",
      "input",
      "aside"
    ]
  },
  "MD036": false,
  "fenced-code-language": false,
  "first-heading-h1": false,
  "first-line-heading": false,
  "no-duplicate-header": false,
  "no-hard-tabs": false,
  "ol-prefix": false,
  "whitespace": false
}
